#!/bin/sh
while read file
do
        case "$1" in
		"d")
			rm "$file" ;;
		"r")
			convert -rotate 90 "$file" "$file" ;;
        	"R")
	      		convert -rotate -90 "$file" "$file" ;;
        	"y")
              		echo -n "$file" | xclip -selection clipboard ;;
        	"w")
              		xwallpaper --zoom "$file" ;;
		"m")
	      		mv "$file" ~ ;;
		"c")
			cp "$file" ~ ;;
	esac
done
