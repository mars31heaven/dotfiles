super + Insert
	clipmenu -l 20

super + w
	$BROWSER

super + e
	$TERMINAL -e $EDITOR

super + p
	dmenu_path_i

super + shift + w
	$TERMINAL -e w3m -B

super + shift + Home
	$TERMINAL -e htop

super + n
	$TERMINAL $EDITOR -c VimwikiIndex

super + r
	$TERMINAL -e newsboat

super + m
	mpdmenu

super + shift + v
	ytfzf -D

super + shift + p
	passmenu

super + shift + d
	todo

super + shift + e
	dmenuconfigs

super + shift + c
	dmenucalc

super + shift + f
	$TERMINAL -e lf

super + F11
	pkill -USR1 mpv

super + shift + F11
	pkill -USR1 mpd

super + F1
	laptopmononly

super + shift + F1
	bothmonitors

super + F2
	extmononly

super + F3
	brownout 500

super + shift + F3
	brownout 0

super + F4
	$TERMINAL -e pulsemixer

super + shift + F4
	pkill pulseaudio

super + F5
	$TERMINAL -e ncmpcpp

super + shift + p
	mpdmenu -p

super + shift + x
	slock

Print
	maim -u ~/pics/screenshots/full_$(date "+%Y_%m_%d_%s").png

shift + Print
	maim -uso ~/pics/screenshots/sel_$(date "+%Y_%m_%d_%s").png

XF86AudioRaiseVolume
	pamixer --increase 5 ; \
	kill -34 "$(cat ~/.cache/pidofbar)"

super + equal
	pamixer --increase 5 ; \
	kill -34 "$(cat ~/.cache/pidofbar)"

XF86AudioLowerVolume
	pamixer --decrease 5 ; \
	kill -34 "$(cat ~/.cache/pidofbar)"

super + minus
	pamixer --decrease 5 ; \
	kill -34 "$(cat ~/.cache/pidofbar)"

XF86AudioMute
	pamixer --toggle-mute ; \
	kill -34 "$(cat ~/.cache/pidofbar)"

super + shift + m
	pamixer --toggle-mute ; \
	kill -34 "$(cat ~/.cache/pidofbar)"

XF86MonBrightnessUp
	xbacklight -inc 2

XF86MonBrightnessDown
	xbacklight -dec 2

XF86AudioPlay
	mpc toggle

XF86AudioStop
	mpc stop

XF86AudioNext
	mpc next

super + Next
	mpc next

super + slash
  	dunstify -h string:x-canonical-private-synchronous:mpd-audio "MPD Notification" "`mpc --format '<b>%title%</b>\nby <i>%artist%</i>\nfrom <i>%album%</i>' current`"

ctrl + space
	dunstctl close-all

super + shift + BackSpace
	dmenupower
