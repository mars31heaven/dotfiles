super + F1
	laptopmononly

super + shift + F1
	bothmons

super + F2
	extmononly

super + F3
	brownout 600

super + shift + F3
	brownout 0

super + F4
	$TERMINAL -e pulsemixer

super + F5
	virt-manager

super + minus
	pamixer --decrease 5 ; \
	kill -34 "$(cat ~/.cache/pidofbar)"

super + plus
	pamixer --increase 5 ; \
	kill -34 "$(cat ~/.cache/pidofbar)"

super + shift + BackSpace
	dmenupower

super + w
	qtb

super + shift + w
	dmenuwireless

super + e
	$TERMINAL -e $EDITOR

super + shift + e
	$TERMINAL -e neomutt

super + p
	dmenu_run_i

super + shift + p
	passmenu

super + shift + a
	$TERMINAL -e abook -C ~/.config/abook/abookrc --datafile ~/.local/share/abook/addressbook

super + shift + s
	$TERMINAL -e spotify-tui

super + d
	$TERMINAL -e $EDITOR -c VimwikiDiaryIndex

super + shift + k
	dmenukill

super + shift + x
	slock

super + c
	dmenucalc

super + v
	$TERMINAL -e pipe-viewer

super + shift + b
	brave

super + n
	$TERMINAL -e ncmpcpp

super + shift + n
	nicotine

super + m
	mpdmenu

super + shift + m
	pamixer --toggle-mute ; \
	kill -34 "$(cat ~/.cache/pidofbar)"

super + shift + less
	mpc prev

super + shift + greater
	mpc next

super + slash
  	dunstify -h string:x-canonical-private-synchronous:mpd-audio "MPD Notification" "`mpc --format '<b>%title%</b>\nby <i>%artist%</i>\nfrom <i>%album%</i>' current`"

ctrl + space
	dunstctl close-all

Print
	maim -u ~/pics/screenshots/full_$(date "+%Y_%m_%d_%s").png

shift + Print
	maim -uso ~/pics/screenshots/sel_$(date "+%Y_%m_%d_%s").png

shift + ctrl + Print
	scrdis

super + Insert
	clipmenu -l 20

super + shift + Home
	$TERMINAL -e htop

XF86Tools
	$TERMINAL -e ncmpcpp

XF86AudioMute
	pamixer --toggle-mute ; \
	kill -34 "$(cat ~/.cache/pidofbar)"

XF86AudioLowerVolume
	pamixer --decrease 5 ; \
	kill -34 "$(cat ~/.cache/pidofbar)"

XF86AudioRaiseVolume
	pamixer --increase 5 ; \
	kill -34 "$(cat ~/.cache/pidofbar)"

XF86MonBrightnessDown
	xbacklight -dec 2

XF86MonBrightnessUp
	xbacklight -inc 2

XF86AudioPlay
	mpc toggle

XF86AudioStop
	mpc stop

XF86AudioPrev
	mpc prev

XF86AudioNext
	mpc next

XF86Mail
	mw -Y

XF86HomePage
	$BROWSER

XF86Calculator
	dmenucalc
